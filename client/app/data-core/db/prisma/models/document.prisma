model Document {
    id        String       @id @default(cuid())
    type      DocumentType
    content   String
    createdAt DateTime     @default(now())
    updatedAt DateTime     @updatedAt
    sessionId String
    session   Session      @relation(fields: [sessionId], references: [id])
}

enum DocumentType {
    SUMMARY
    SOAP
    REFERRAL
    PROGRESS_NOTE
    PRESCRIPTION
    LAB_ORDER
    DAP
    PIE
}

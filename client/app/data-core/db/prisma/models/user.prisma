model User {
    id          String        @id @default(cuid())
    email       String        @unique
    name        String?
    password    String?
    createdAt   DateTime      @default(now())
    updatedAt   DateTime      @updatedAt
    userSession UserSession[]
    Session     Session[]
    UserSession    UserTemplate[]
}

model UserSession {
    id        String   @id @default(cuid())
    userId    String
    token     String   @unique
    user      User     @relation(fields: [userId], references: [id])
    expiresAt DateTime
    createdAt DateTime @default(now())
}
    
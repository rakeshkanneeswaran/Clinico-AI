model Patient {
    id        String   @id @default(cuid())
    name      String   @default("Not Provided")
    age       String   @default("Not Provided")
    gender    String   @default("Not Provided")
    weight    String   @default("Not Provided")
    height    String   @default("Not Provided")
    bloodType String   @default("Not Provided")
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    sessionId String   @unique
    session   Session  @relation(fields: [sessionId], references: [id])
}
